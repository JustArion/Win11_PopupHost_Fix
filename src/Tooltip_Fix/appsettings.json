{
  "Serilog" : {
    "Using" : [],
    "MinimumLevel" : {
      "Default" : "Verbose",
      "Override" : {
        "Microsoft" : "Warning",
        "System" : "Warning"
      }
    },
    "Enrich" : [ "WithProcessName", "FromLogContext" ],
    "WriteTo" : [
      {
        "Name" : "Console",
        "Args" :
        {
          "outputTemplate" : "{Level:u1} {Timestamp:yyyy-MM-dd HH:mm:ss.ffffff} [{Source}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name" : "Seq",
        "Args" :
        {
          "serverUrl" : "http://localhost:9999"
        }
      }
    ]


  }
}